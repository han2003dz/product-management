mixin alert-success(time)
  if messages.success
    .message.info.col-4
      .alert.alert-success.fs-6(
        show-alert 
        data-time=time
      ) #{messages.success} <span close-alert>x</span>
  if messages.danger
    .message.info.col-4
      .alert.alert-danger.fs-6(
        show-alert 
        data-time=time
      ) #{messages.danger} <span close-alert>x</span>